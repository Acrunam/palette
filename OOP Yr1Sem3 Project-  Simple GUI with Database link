    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
       // TODO add your handling code here:
       //validation
        String stru="";
        stru=jTextField1.getText();
        
        String strp="";
        strp=jPasswordField1.getText();
        if (stru.isEmpty()==true)
        {
         JOptionPane.showMessageDialog(null,"Enter User Name");
         return;
        }
        
        if (strp.isEmpty()==true)
        {
         JOptionPane.showMessageDialog(null,"Enter Password");
         return;
        }
        try
        {
            //get database connection details
            AmsApplicationUI mc=new AmsApplicationUI();
            
             //open connection
            String url= "jdbc:mysql://localhost/systemusers";
Connection con = DriverManager.getConnection(url,"root","");
            String str="";
            str="select * from login where username =? and password =?";
           PreparedStatement pst=con.prepareStatement(str);
           pst.setString(1, stru);
           pst.setString(2, strp);
           ResultSet rs;
           rs=pst.executeQuery();
           if (rs.next())
           {
               AmsApplicationUI.StrUser=jTextField1.getText();
               CourseEvalUI m=new CourseEvalUI();
               m.setVisible(true);
               this.setVisible(false);
               
           }
           else
           {
                JOptionPane.showMessageDialog(null,"User name or password are not correct.");
                return;
            }
           

        }
        catch (Exception e)
        {
            System.err.println(e);
            System.exit(1);
        }
    }//GEN-LAST:event_jButton1ActionPerformed

